#!/bin/bash

NRP=TRUE
PREFIX=MON
STAGING_TYPE=LIVE-STAGING

while [[ $# -gt 0 ]]; do
    case "$1" in 
        -n|--no-modules)
            NRP=FALSE
            shift
            ;;
        -v|--version)
            echo "Version 0.1"
            exit 0
            ;;
        -c|--canada)
            PREFIX=MCA
            shift
            ;;
        -s|--staging)
            STAGING_TYPE=STAGING
            shift
            ;;
        -u|--uk)
            PREFIX=MUK
            shift
            ;;
        -h|--help)
            echo help()
            exit 0
            ;;
        -l|--list)
            eb list
            exit 0
            ;;
        *)
            if [[ "$1" =~ ^[0-9]+$ ]]; then
                STAGING_ENV=$1
            else
                echo "Invalid Option: $1"
                exit 1
                ;;
            fi
    esac
done


if [[ $NRP ]]




